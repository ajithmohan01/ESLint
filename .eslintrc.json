{
    "extends": "airbnb-base",
    "rules": {
        "no-restricted-imports": ["error", "underscore"],
        "eqeqeq": "error",
        "no-console": "off",
        "consistent-return": "error",
        "no-unused-vars": "off",
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"]
    }

    
  }
